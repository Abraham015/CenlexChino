{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "project": "cz-chino",
    "rules": {
      "rules": {
        ".read": "auth == null",
        ".write": "auth != null"
      }
    }
  }
}
